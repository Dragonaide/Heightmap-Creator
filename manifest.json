{
  
  "manifest_version": 2,
  "name": "Heightmap Creator",
  "version": "0.0.1",
  
  "description": "Create heightmaps anytime, anywhere.",
  
  "browser_action": {
    "default_icon": "Components/ExtensionIcon.svg"
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  
}
